    <xs:complexType name="RaceDayCalendar">
        <xs:annotation>
            <xs:documentation>Calendar caontaining all the racedays available under a certain period</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="fromDate" type="com:AtgDate">
                <xs:annotation>
                    <xs:documentation>Startdate in the calendar</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="toDate" type="com:AtgDate">
                <xs:annotation>
                    <xs:documentation>Enddate in the calendar</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="raceDayInfos" type="RaceDayInfo" nillable="false" minOccurs="0"
                maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>All the racedays in the period</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
