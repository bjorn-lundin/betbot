    <xs:complexType name="RaceResult">
        <xs:annotation>
            <xs:documentation>Complete result for a race, not tote</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="RaceBase">
                <xs:sequence>
                    <xs:element name="starts" type="StartResult">
                        <xs:annotation>
                            <xs:documentation>All the starts in the race.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="winMargin" type="xs:string">
                        <xs:annotation>
                            <xs:documentation>Difference in length between the first placed horse and the second placed horse.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="StartResult">
        <xs:annotation>
            <xs:documentation>Holds in addition to basic start info, result specific infomation.</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="StartBase">
                <xs:sequence>
                    <xs:element name="odds" type="xs:string">
                        <xs:annotation>
                            <xs:documentation>The odds that the the horse would have paid in case of a victory. This is presented as the odds multiplied by 10. Will contain 25 if the final odds would have been 2,5.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="place" type="PlaceInfo">
                        <xs:annotation>
                            <xs:documentation>The place information for the horse in the race.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="finishingPosition" type="xs:int">
                        <xs:annotation>
                            <xs:documentation>The order in which the horse passed the finish line. Unplaced horses also have a
finishing position. </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="earning" type="com:Amount">
                        <xs:annotation>
                            <xs:documentation>Amount of money that the horse earned in the race.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="time" type="ResultRowTime" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation>Finishing time for the horse in the race, (also a gallopflag and startmethodinfo).</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="shoeInfo" type="com:ShoeInfo">
                        <xs:annotation>
                            <xs:documentation>Holds information whether or not the horse wore shoes in the race. The information is missing if not known. Only valid for Trot/Harness racing.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
