	<xs:complexType name="DefaultRaceEvent">
		<xs:annotation>
			<xs:documentation>
Notifies which race number is next to run and when the race is about to start.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="Event">
				<xs:sequence>
					<xs:element name="raceDaydate" type="com:AtgDate"/>
					<xs:element name="trackKey" type="com:TrackKey">
						<xs:annotation>
							<xs:documentation>Identifies a track.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="raceNumber" type="xs:int">
						<xs:annotation>
							<xs:documentation>
Race number in the raceday that is default for betting. ie which race number that is next to run. 1-15.
                            </xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="timeToStart" type="com:AtgDateTime">
						<xs:annotation>
							<xs:documentation>
When the race is about to start, local time.
                            </xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="currentTime" type="com:AtgDateTime">
						<xs:annotation>
							<xs:documentation>Current server time, local, to calculate minutes left to start.
                            </xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="timeToStartUTC" type="com:AtgDateTime">
						<xs:annotation>
							<xs:documentation>
When the race is about to start (UTC).
                            </xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="currentTimeUTC" type="com:AtgDateTime">
						<xs:annotation>
							<xs:documentation>Current server time in UTC, to calculate minutes left to start.
                            </xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="TrackConditionEvent">
		<xs:annotation>
			<xs:documentation>Notifies that the track condition for a track at a specific date has changed.
            </xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="Event">
				<xs:sequence>
					<xs:element name="raceDaydate" type="com:AtgDate"/>
					<xs:element name="trackKey" type="com:TrackKey">
						<xs:annotation>
							<xs:documentation>Identifies a track.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="trackCondition" type="com:TrackCondition"
						maxOccurs="unbounded" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Last known track condition for the track.
Possible values Galop(code/text/english text):
nt/Något tung/                         
sn/Snabb/                              
st/Standard/                           
tg/Tung/
Possible values Trot(code/text/english text):
L/Lätt bana/                          
N/Något tung bana/                    
S/Struket lopp som ej omköres/        
T/Tung bana/                          
V/Vinterbana/</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="timestamp" type="com:AtgDateTime">
						<xs:annotation>
							<xs:documentation>Time when the information was generated.</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
