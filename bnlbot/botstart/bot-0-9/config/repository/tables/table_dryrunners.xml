<?xml version="1.0" encoding="ISO-8859-15"?>
<MaAstro>
    <Table Desc="collected runners " Name="DRYRUNNERS" Tablespace="" >
        <Column Name="marketid"           Size="9"  Type="2" AllowNull="0"  Description="market id"/>
        <Column Name="selectionid"        Size="9"  Type="2" AllowNull="0"  Description="selection_id"/>
        <Column Name="index"              Size="9"  Type="2" AllowNull="1"  Description="index"/>
        <Column Name="backprice"          Size="8"  Type="6" AllowNull="0"  Description="back_price"/>
        <Column Name="layprice"           Size="8"  Type="6" AllowNull="0"  Description="lay_price"/>
        <Column Name="runnername"         Size="50" Type="1" AllowNull="0"  Description="runner_name"/>
        <Column Name="runnernamestripped" Size="50" Type="1" AllowNull="1"  Description="runner_name without startnum"/>
        <Column Name="startnum"           Size="2"  Type="1" AllowNull="1"  Description="startnum from runnername"/>
        <Index Columns="marketid,selectionid" type="primary"/>
        <Index Columns="runnernamestripped" type="index"/>
    </Table>
</MaAstro>

