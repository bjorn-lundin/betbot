+
>&2
-- MAIN=yes >/tmp/gnat_trace_main_$$.log
-- POSIX1=no >/tmp/gnat_trace_posix_$$.log
-- TOKEN=yes >/tmp/gnat_trace_token_$$.log
-- LOCK=yes >/tmp/gnat_trace_lock_$$.log

--  decorators 
DEBUG.COLORS=yes
DEBUG.ABSOLUTE_TIME=yes
DEBUG.COUNT=no
DEBUG.ELAPSED_TIME=yes

--  Applies to FIRST.EXCEPTIONS, LAST.EXCEPTIONS,...
--  and forces them to be displayed on stdout
*.EXCEPTIONS=yes > stdout

