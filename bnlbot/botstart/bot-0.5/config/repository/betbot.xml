<Process>
  <paths>
    <tables>
      <definitions>
        <directory>$BETBOT_CONFIG/tables</directory>
        <pattern>table_*.xml</pattern>
        <prefix>table</prefix>
      </definitions>
    </tables>

    <clreqs>
      <definitions>
        <directory>$BETBOT_CONFIG/requests</directory>
        <pattern>req_*.xml</pattern>
        <prefix>req</prefix>
      </definitions>
    </clreqs>

    <terms>
      <definitions>
        <directory>$BETBOT_CONFIG/terms</directory>
        <pattern>trm_*.xml</pattern>
        <prefix>trm</prefix>
      </definitions>
    </terms>

    <codes>
      <definitions>
        <directory>$BETBOT_CONFIG/codes</directory>
        <pattern>trm_*.xml</pattern>
        <prefix>trm</prefix>
      </definitions>
    </codes>

    <labels>
      <definitions>
        <directory>$BETBOT_CONFIG/labels</directory>
        <pattern>lab_*.xml</pattern>
        <prefix>lab</prefix>
      </definitions>
    </labels>

    <views>
      <definitions>
        <directory>$BETBOT_CONFIG/views</directory>
        <pattern>view_*.xml</pattern>
        <prefix>view</prefix>
      </definitions>
    </views>
    <repositoryRoot>
      <definitions>
        <directory>$BETBOT_CONFIG/</directory>
      </definitions>
    </repositoryRoot>
    <bin>
      <definitions>
        <directory>$$BETBOT_CONFIG/</directory>
      </definitions>
    </bin>
    <IDFRoot>
      <definitions>
        <directory>$BETBOT_CONFIG/</directory>
      </definitions>
    </IDFRoot>

    <mustang>
      <definitions>
        <module>$BETBOT_CONFIG/m2_modules.xml</module>
        <r-menu>$BETBOT_CONFIG/m2_r_menus.xml</r-menu>
      </definitions>
    </mustang>

</paths>


</Process>


