<?xml version="1.0" encoding="ISO-8859-15"?>
<BnlBot xmlns="http://www.nonodev.com/bnlbot" xmlns:xcl="http://www.nonodev.com/bnlbot" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <Table Wiz="3" Desc="Probailtity to win header" Name="APROBR" Tablespace="">
        <Column Name="betid"          AllowNull="0" Description="bet id"/>
        <Column Name="markettype"     AllowNull="0" Description="market id"/>
        <Column Name="rank"           AllowNull="0" Description="where in race currently"/>
        <Column Name="selectionid"    AllowNull="0" Description="selection id"/>
        <Column Name="r1limit"        AllowNull="0" Description="max leader price"/>
        <Column Name="backprice"      AllowNull="0"  Description="back price"/>
        <Column Name="layprice"       AllowNull="0"  Description="lay price"/>
        <Column Name="backprice1"     AllowNull="0"  Description="back price"/>
        <Column Name="layprice1"      AllowNull="0"  Description="lay price"/>
        <Column Name="betplaced"      AllowNull="1" Description="when bet was placed"/>
        <Column Name="wonrace"        AllowNull="0" Description="won"/>
        <Column Name="placedrace"     AllowNull="0" Description="placed"/>
        <Column Name="svnrevision"    AllowNull="0" Description="svn revision of bot"/>
        <Column Name="ixxlupd"        AllowNull="0" Description="Latest updater"/>
        <Column Name="ixxluts"        AllowNull="0" Description="Latest update timestamp"/>
        <Index Columns="betid,markettype,rank" type="primary"/>
        <Index Columns="markettype" type="index"/>
        <Index Columns="rank" type="index"/>
        <Index Columns="selectionid" type="index"/>
    </Table>
</BnlBot>
