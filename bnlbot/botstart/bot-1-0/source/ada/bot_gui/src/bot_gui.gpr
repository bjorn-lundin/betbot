with "gnoga";
--with "../../adabot";

project bot_gui extends "../../adabot" is
   for Languages use ("Ada");
   for Source_Dirs use (".");
   for Object_Dir use "../obj";
   for Exec_Dir use "../bin";
   for Main use ("bot_gui-main.adb");

   package Binder is
     for Default_Switches ("ada") use ("-E");
   end Binder;

   package Builder is
      for Executable ("bot_gui-main.adb") use "bot_gui";
   end Builder;
end bot_gui;
