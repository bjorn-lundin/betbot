                        Namespace_URI : Unicode.CES.Byte_Sequence := "";
                        Local_Name    : Unicode.CES.Byte_Sequence := "";
                        Qname         : Unicode.CES.Byte_Sequence := "") is
    pragma Warnings(Off,Namespace_URI);
    pragma Warnings(Off,Qname);
    The_Tag : constant String := Local_Name;
  begin
    if The_Tag = Table_Dry_markets_Set_Name then
      Handler.Found_Set := false;
    elsif The_Tag = Table_Dry_markets_Row_Name then
      if Handler.Found_Set then
        Table_Dry_markets.Dry_markets_List_Pack.Insert_At_Tail(Handler.Dry_markets_List, Handler.Dry_markets_Data);
        Handler.Dry_markets_Data := Empty_Data;
      end if;
