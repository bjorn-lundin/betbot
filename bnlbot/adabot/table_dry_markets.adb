  procedure Characters(Handler          : in out Dry_markets_Reader;
                       Ch               : Unicode.CES.Byte_Sequence := "") is
    function To_Iso_Latin_15(Str : Unicode.CES.Byte_Sequence) return String is
      use Unicode.Encodings;
    begin
      return  Convert(Str, Get_By_Name("utf-8"),Get_By_Name("iso-8859-15"));
    end To_Iso_Latin_15;
    The_Tag   : constant String := To_String(Handler.Current_Tag);
    The_Value : constant string := To_Iso_Latin_15(Ch);
    procedure Fix_String (Value    : string;
                          Variable : in out string) is
    begin
      Append(Handler.Accumulated, The_Value);
      Ada.Strings.Fixed.Move(To_String(Handler.Accumulated), Variable);
    end Fix_String;
  begin
    if Handler.Found_Set then
      if    The_Tag = MARKET_ID_Name then 
        Handler.Dry_markets_Data.Market_id := Integer_4'value(The_Value);
      elsif The_Tag = BSP_MARKET_Name then 
        Fix_String(The_Value, Handler.Dry_markets_Data.Bsp_market);
      elsif The_Tag = MARKET_TYPE_Name then 
        Fix_String(The_Value, Handler.Dry_markets_Data.Market_type);
      elsif The_Tag = EVENT_HIERARCHY_Name then 
        Fix_String(The_Value, Handler.Dry_markets_Data.Event_hierarchy);
      elsif The_Tag = LAST_REFRESH_Name then 
        Handler.Dry_markets_Data.Last_refresh := Sattmate_Calendar.To_Time_Type(The_Value(1..11), The_Value(13..24));
      elsif The_Tag = TURNING_IN_PLAY_Name then 
        Fix_String(The_Value, Handler.Dry_markets_Data.Turning_in_play);
      elsif The_Tag = MENU_PATH_Name then 
        Fix_String(The_Value, Handler.Dry_markets_Data.Menu_path);
      elsif The_Tag = BET_DELAY_Name then 
        Handler.Dry_markets_Data.Bet_delay := Integer_4'value(The_Value);
      elsif The_Tag = EXCHANGE_ID_Name then 
        Handler.Dry_markets_Data.Exchange_id := Integer_4'value(The_Value);
      elsif The_Tag = COUNTRY_CODE_Name then 
        Fix_String(The_Value, Handler.Dry_markets_Data.Country_code);
      elsif The_Tag = MARKET_NAME_Name then 
        Fix_String(The_Value, Handler.Dry_markets_Data.Market_name);
      elsif The_Tag = MARKET_STATUS_Name then 
        Handler.Dry_markets_Data.Market_status := Integer_4'value(The_Value);
      elsif The_Tag = EVENT_DATE_Name then 
        Handler.Dry_markets_Data.Event_date := Sattmate_Calendar.To_Time_Type(The_Value(1..11), The_Value(13..24));
      elsif The_Tag = NO_OF_RUNNERS_Name then 
        Handler.Dry_markets_Data.No_of_runners := Integer_4'value(The_Value);
      elsif The_Tag = TOTAL_MATCHED_Name then 
        Handler.Dry_markets_Data.Total_matched := Integer_4'value(The_Value);
      elsif The_Tag = NO_OF_WINNERS_Name then 
        Handler.Dry_markets_Data.No_of_winners := Integer_4'value(The_Value);
      end if;
    end if;
  exception
    when Ada.Strings.Length_Error => Handler.OK := False;
  end Characters;

