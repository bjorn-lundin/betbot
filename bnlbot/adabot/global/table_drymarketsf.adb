  procedure Characters(Handler          : in out Drymarketsf_Reader;
                       Ch               : Unicode.CES.Byte_Sequence := "") is
    function To_Iso_Latin_15(Str : Unicode.CES.Byte_Sequence) return String is
      use Unicode.Encodings;
    begin
      return  Convert(Str, Get_By_Name("utf-8"),Get_By_Name("iso-8859-15"));
    end To_Iso_Latin_15;
    The_Tag   : constant String := To_String(Handler.Current_Tag);
    The_Value : constant string := To_Iso_Latin_15(Ch);
    procedure Fix_String (Value    : string;
                          Variable : in out string) is
    begin
      Append(Handler.Accumulated, The_Value);
      Ada.Strings.Fixed.Move(To_String(Handler.Accumulated), Variable);
    end Fix_String;
  begin
    if Handler.Found_Set then
      if    The_Tag = MARKETID_Name then 
        Handler.Drymarketsf_Data.Marketid := Integer_4'value(The_Value);
      elsif The_Tag = BSPMARKET_Name then 
        Fix_String(The_Value, Handler.Drymarketsf_Data.Bspmarket);
      elsif The_Tag = MARKETTYPE_Name then 
        Fix_String(The_Value, Handler.Drymarketsf_Data.Markettype);
      elsif The_Tag = EVENTHIERARCHY_Name then 
        Fix_String(The_Value, Handler.Drymarketsf_Data.Eventhierarchy);
      elsif The_Tag = LASTREFRESH_Name then 
        Handler.Drymarketsf_Data.Lastrefresh := Sattmate_Calendar.To_Time_Type(The_Value(1..11), The_Value(13..24));
      elsif The_Tag = TURNINGINPLAY_Name then 
        Fix_String(The_Value, Handler.Drymarketsf_Data.Turninginplay);
      elsif The_Tag = MENUPATH_Name then 
        Fix_String(The_Value, Handler.Drymarketsf_Data.Menupath);
      elsif The_Tag = BETDELAY_Name then 
        Handler.Drymarketsf_Data.Betdelay := Integer_4'value(The_Value);
      elsif The_Tag = EXCHANGEID_Name then 
        Handler.Drymarketsf_Data.Exchangeid := Integer_4'value(The_Value);
      elsif The_Tag = COUNTRYCODE_Name then 
        Fix_String(The_Value, Handler.Drymarketsf_Data.Countrycode);
      elsif The_Tag = MARKETNAME_Name then 
        Fix_String(The_Value, Handler.Drymarketsf_Data.Marketname);
      elsif The_Tag = MARKETSTATUS_Name then 
        Handler.Drymarketsf_Data.Marketstatus := Integer_4'value(The_Value);
      elsif The_Tag = EVENTDATE_Name then 
        Handler.Drymarketsf_Data.Eventdate := Sattmate_Calendar.To_Time_Type(The_Value(1..11), The_Value(13..24));
      elsif The_Tag = NOOFRUNNERS_Name then 
        Handler.Drymarketsf_Data.Noofrunners := Integer_4'value(The_Value);
      elsif The_Tag = TOTALMATCHED_Name then 
        Handler.Drymarketsf_Data.Totalmatched := Integer_4'value(The_Value);
      elsif The_Tag = NOOFWINNERS_Name then 
        Handler.Drymarketsf_Data.Noofwinners := Integer_4'value(The_Value);
      end if;
    end if;
  exception
    when Ada.Strings.Length_Error => Handler.OK := False;
  end Characters;

