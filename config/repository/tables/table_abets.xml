<?xml version="1.0" encoding="ISO-8859-15"?>
<BnlBot xmlns="http://www.nonodev.com/bnlbot" xmlns:xcl="http://www.nonodev.com/bnlbot" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <Table Wiz="3" Desc="Bets" Name="ABETS" Tablespace="big">
        <Column Name="betid"          AllowNull="0" Description="bet id"/>
        <Column Name="marketid"       AllowNull="0" Description="market id"/>
        <Column Name="betmode"        AllowNull="0" Description="dry,real,sim"/>
        <Column Name="powerdays"      AllowNull="0" Description="power of denominator"/>
        <Column Name="selectionid"    AllowNull="0" Description="selection id"/>
        <Column Name="reference"      AllowNull="0" Description="reference"/>
        <Column Name="size"           AllowNull="0" Description="wanted size"/>
        <Column Name="price"          AllowNull="0" Description="wanted price"/>
        <Column Name="side"           AllowNull="0" Description="BACK/LAY"/>
        <Column Name="betname"        AllowNull="0" Description="name of bet"/>
        <Column Name="betwon"         AllowNull="1" Description="success"/>
        <Column Name="profit"         AllowNull="0" Description="vinst"/>
        <Column Name="status"         AllowNull="0" Description="status"/>
        <Column Name="exestatus"      AllowNull="0" Description="execution status"/>
        <Column Name="exeerrcode"     AllowNull="0" Description="execution errcode"/>
        <Column Name="inststatus"     AllowNull="0" Description="intruction status"/>
        <Column Name="insterrcode"    AllowNull="0" Description="intruction errcode"/>
        <Column Name="startts"        AllowNull="1" Description="start ts according to market"/>
        <Column Name="betplaced"      AllowNull="1" Description="when bet was placed"/>
        <Column Name="pricematched"   AllowNull="0" Description="matched price"/>
        <Column Name="sizematched"    AllowNull="0" Description="size price"/>
        <Column Name="runnername"     AllowNull="0" Description="name of runner"/>
        <Column Name="fullmarketname" AllowNull="0" Description="name of market"/>
        <Column Name="svnrevision"    AllowNull="0" Description="svn revision of bot"/>
        <Column Name="ixxlupd"        AllowNull="0" Description="Latest updater"/>
        <Column Name="ixxluts"        AllowNull="0" Description="Latest update timestamp"/>
        <Index Columns="betid" type="primary"/>
        <Index Columns="marketid" type="index"/>
        <Index Columns="betwon" type="index"/>
        <Index Columns="powerdays" type="index"/>
        <Index Columns="betplaced" type="index"/>
        <Index Columns="startts" type="index"/>
        <Index Columns="betname" type="index"/>
        <Index Columns="status" type="index"/>
        <Index Columns="reference" type="index"/>
        <Index Columns="date_part('year'::text, betplaced)" type="functional"/>
        <Index Columns="date_part('month'::text, betplaced)" type="functional"/>
        <Index Columns="date_part('day'::text, betplaced)" type="functional"/>
    </Table>
</BnlBot>
