<?xml version="1.0" encoding="ISO-8859-15"?>
<BnlBot xmlns="http://www.nonodev.com/bnlbot" xmlns:xcl="http://www.nonodev.com/bnlbot" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <Table Desc="runners odds at race" Name="ARACEPRICES" Tablespace="big" >
        <Column Name="pricets"     AllowNull="0" Description="timestamp of prices"/>
        <Column Name="marketid"    AllowNull="0"  Description="market id"/>
        <Column Name="selectionid" AllowNull="0"  Description="selection_id"/>
        <Column Name="status"      AllowNull="0" Description="status"/>
        <Column Name="backprice"   AllowNull="0"  Description="back_price"/>
        <Column Name="layprice"    AllowNull="0"  Description="lay_price"/>
        <Column Name="ixxlupd"     AllowNull="0" Description="Latest updater"/>
        <Column Name="ixxluts"     AllowNull="0" Description="Latest update timestamp"/>
        <Index Columns="pricets,marketid,selectionid" type="primary"/>
        <Index Columns="selectionid" type="index"/>
        <Index Columns="backprice" type="index"/>
        <Index Columns="layprice" type="index"/>
        <Index Columns="status" type="index"/>
        <Index Columns="marketid" type="index"/>
        <Index Columns="pricets" type="index"/>
        <Index Columns="ixxluts" type="index"/>
    </Table>
</BnlBot>
